{
"manifest_version": 3,
    "name": "Swift Savings",
    "version": "1.0",
    "description": "This extension compares prices for manga and figurines from various sites",
    "permissions": [
        "activeTab",
        "storage",
        "scripting",
        "http://localhost:5000/*"
    ],
    "icons":{ 
        "16": "Swift_page/images/Slime_0.png",
        "48": "Swift_page/images/Slime_0.png",
        "128": "Swift_page/images/Slime_0.png"
    },
    "action": {
        "default_popup": "index.html",
        "default_icon": {
        "16": "Swift_page/images/Slime_0.png",
        "48": "Swift_page/images/Slime_0.png",
        "128": "Swift_page/images/Slime_0.png"
        }
    },
    "content_scripts": [
        {
            "matches":["<all_urls>"],
            
            "js": ["Swift_page/src/server.js"]
        }
    ]
}